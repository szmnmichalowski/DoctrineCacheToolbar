{
    "name": "szmnmichalowski/doctrine-cache-toolbar",
    "description": "Statistics for Second Level Cache shown on ZendDeveloperTools",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "doctrine",
        "doctrine2",
        "extension",
        "cache",
        "toolbar"
    ],
    "homepage": "https://github.com/szmnmichalowski/DoctrineCacheToolbar",
    "authors": [
        {
            "name": "Szymon Micha≈Çowski",
            "email": "szmnmichalowski@gmail.com"
        }
    ],
    "require": {
        "php": "^5.6 || ^7",
        "doctrine/doctrine-orm-module": "^1.1 || ^2.1",
        "zendframework/zend-developer-tools": "^1.0 || ^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.0 || ^5.0 || ^6.0 || ^7.0",
        "container-interop/container-interop": "^1.1",
        "zendframework/zend-test": "^2.5 || ^3.2"
    },
    "autoload": {
        "psr-4": {
            "DoctrineCacheToolbar\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DoctrineCacheToolbarTest\\": "test/"
        }
    }
}
